import type { Event } from "@/prisma/generated";

export function createEventMock(overrides: Partial<Event> = {}): Event {
  return {
    id: 1,
    name: "テストイベント",
    startDate: new Date(),
    headerImageURL: "",
    tenantId: 1,
    createdAt: new Date(),
    updatedAt: new Date(),
    ...overrides,
  };
}
