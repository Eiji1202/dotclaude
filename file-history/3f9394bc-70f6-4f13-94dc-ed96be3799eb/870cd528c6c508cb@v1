import type { CheckinTransaction } from "@/prisma/generated";

export function createCheckinTransactionMock(
  overrides: Partial<CheckinTransaction> = {},
): CheckinTransaction {
  return {
    id: 1,
    userId: "user-id",
    eventId: 1,
    checkinCodeId: "test-code",
    receivedAt: new Date(),
    createdAt: new Date(),
    updatedAt: new Date(),
    ...overrides,
  };
}
